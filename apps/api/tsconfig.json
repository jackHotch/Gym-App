{
  "extends": "@gymapp/typescript-config/base.json",
  "compilerOptions": {
    "target": "es2022",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "module": "commonjs",
    "moduleResolution": "node",
  },
  "include": ["src/**/*.ts", "./api/*.ts"],
  "exclude": ["jest.config.ts"]
}
